{
    "name": "codefog/contao-news_categories",
    "description": "news_categories extension for Contao Open Source CMS",
    "keywords": ["contao", "news", "categories"],
    "type": "contao-module",
    "license": "LGPL-3.0+",
    "authors": [
        {
            "name": "Codefog",
            "homepage": "http://codefog.pl"
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "contao/core-bundle": "^3.5.9 | ^4.2.0",
        "contao-community-alliance/composer-plugin": "~2.4 | ~3.0",
        "codefog/contao-widget_tree_picker": "^2.0.1"
    },
    "suggest": {
        "terminal42/dc_multilingual": "2.*"
    },
    "extra": {
        "contao": {
            "sources": {
                "": "system/modules/news_categories"
            }
        }
    },
    "replace": {
        "contao-legacy/news_categories": "self.version"
    }
}
